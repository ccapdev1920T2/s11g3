<div class="col-xl-2 offset-xl-0" style="margin: 0px 0px 30px 0px;height: 270px;margin-top: 1%;margin-right: 15px;margin-left: 15px;">
	<a href="{{{getPLink code 1}}}">
		<div class="card-img-overlay" style="height: auto;width: auto;padding: 1px;margin-right: 1%;margin-left: 1%;">
			<div style="width: auto;height: auto;">
				<img src="{{{getPImg filename}}}" style="width: 184px;height: 228px;">
			</div>
			<div>
				<h4 class="text-left" style="margin-top: 2px;color: rgb(0,0,0);font-size: 14px;margin-bottom: 0px;margin-left: 0px;margin-right: 0px;width: 184px;">{{name}} : {{code}}</h4>
			</div>
			<div class="productContainer" style="width: 184px;height: 21px;margin-top: 1px;">
				<div style="width: 140px;">
					<h4 class="text-left float-left" style="color: rgb(119,123,114);font-size: 14px;margin-bottom: 0;">Php {{getPrice price}}</h4>
				</div>
				<div class="text-right" style="width: 44px;align-content: flex-end;">
					<h4 class="text-right float-right" style="color: rgb(119,123,114);font-size: 14px;width: 33px;margin-bottom: 0;margin-right: -108px;">{{qty}}</h4>
				</div>
			</div>
		</div>
	</a>
</div>
